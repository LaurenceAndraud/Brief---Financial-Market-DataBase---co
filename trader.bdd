CREATE DATABASE finance;

CREATE TABLE finance.Trader
(
    nom VarChar(30),
    classe_actif VarChar(30),
    anneeExperience int,
    nomEquipe VarChar(30),
    PRIMARY KEY (nom),
    FOREIGN KEY (nomEquipe) REFERENCES finance.nomEquipe(nom)
);

