CREATE TABLE finance.Transaction
(
    nom VarChar(30),
    nomEquipe VarChar(30),
    dates int,
    lieu VarChar(30),
    prix int,
    PRIMARY KEY (nom),
    FOREIGN KEY (nomEquipe) REFERENCES nomEquipe(nomEquipe)
);

